  "github_url": "https://github.com/adamgraham/STween",
  "readme": "README.md",
  
  "source_directory": "./STween",
  "min_acl": "internal",
  "skip_undocumented": true,

  "exclude": [
    "./STween/STween/Easing/EaseMappings.swift",
    "./STween/STween/Tweening/Conformance/CALayer+Tweening.swift",
    "./STween/STween/Tweening/Conformance/NSLayoutConstraint+Tweening.swift",
    "./STween/STween/Tweening/Conformance/UIButton+Tweening.swift",
    "./STween/STween/Tweening/Conformance/UILabel+Tweening.swift",
    "./STween/STween/Tweening/Conformance/UITextField+Tweening.swift",
    "./STween/STween/Tweening/Conformance/UIView+Tweening.swift",
    "./STween/STween/Utils/DoubleConvertible.swift",
    "./STween/STween/Utils/FloatingPoint+Arithmetic.swift",
    "./STween/STween/Utils/Functions.swift",
    "./STween/STween/Utils/Color/RGBA.swift",
    "./STween/STween/Utils/Color/UIColor+RGBA.swift"
  ],

  "custom_categories": [
    {
      "name": "Easing",
      "children": [
        "Ease",
        "EaseAlgorithm",
        "EaseAlgorithms",
        "EaseClassification",
        "EaseCurve"
      ]
    },
    {
      "name": "Interpolation",
      "children": [
        "Interpolatable",
        "InterpolationValues"
      ]
    },
    {
      "name": "Tweening",
      "children": [
        "Tween",
        "TweenAnimation",
        "TweenError",
        "TweenState",
        "TweenStateChange",
        "TweenTimer",
        "TweenTimerDelegate",
        "Tweenable",
        "TweenableProperty",
        "Tweener"
      ]
    },
    {
      "name": "Settings",
      "children": [
        "Defaults",
        "FrameRate"
      ]
    }
  ]
}